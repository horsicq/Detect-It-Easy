function detect(bShowType, bShowVersion, bShowOptions) {
    if (PE.findSignature(PE.getSize() - 0x100, 0x100, "8b042483e8..68........ffd0b8........3d..............30..40eb..b8........3d..............30..40eb") != -1) {
        bDetected = true;
    }

    return result(bShowType, bShowVersion, bShowOptions);
}