function isProtoTracker() {
	if(!X.c("'SONG'") ) return;
	//ref https://github.com/kometbomb/prototracker/blob/master/doc/FORMAT.TXT
	//TODO tighten somewhat
	sz = X.U32(4,_LE); p = 8;
	v = X.U8(p++); trk = v >= 1? X.U8(p++):4; fxprm = v >= 17? X.U8(p++): 1;
	fxc = v >= 17? X.U8(p++): 0;
	title = X.SC(p,0x1000,'CP1250'); p += title.length+1;
	ptnr = X.U8(p++)+1; seqr = X.U8(p++)+1;
	if(!ptnr || !seqr) return; mcr = 0;
	for(i=eof=0; p < X.Sz() && !eof; p += X.U32(p+4)) {
		switch(X.SA(p,4)) {
		case 'SEQU': if(X.U8(p+8) > seqr) return; i |= 1; break;
		case 'PATT': i |= 0x10; break;
		case 'MACR': i |= 0x100; mcr = X.U8(p+8); break;
		default: if(i < 0x11) return;  eof = 1
		}
	}
	sz = p;
	return true
}
