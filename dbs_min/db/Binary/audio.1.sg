function isSonicArranger() {
	//from https://gitlab.com/uade-music-player/uade/-/raw/master/amigasrc/players/wanted_team/Sonic_Arranger/Sonic Arranger_v1.asm
	p = modp = smp = synsmp = ord = ptn = 0; x = 1; sv = bad = ''; owner = [];
