		if(nV >= 0x101 && X.U8(sz) == 0xFF) sz++; //a lot of v1.1 CMFs end in FF, enough to be significant?
		sOption('ch:'+ch+' ins:'+ins+' sz:'+outSz(sz))
	}
}


function isDSym() {
