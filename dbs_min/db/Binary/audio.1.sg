	}
	sz = 0x200+allsmpsz+ord*0x10;
	if(!old) sz += X.U8(0x1D)*0x40; //what IS this value?
	ptn = 0;
	for(i=0; i < ord*16; i+=4) { p = X.U16(0x200+i,_BE); if(p>ptn) ptn = p }
	sz += ptn*0x30;
	return true
}
