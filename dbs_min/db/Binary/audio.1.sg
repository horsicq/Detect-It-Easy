	if(X.Sz() < 90 || !X.c("02011313 1412010B") || X.U8(8) > 1 || !isWithin(ch=X.U8(9), 1,8)
	  || (ord=X.U16(0xA)) > 4096 || (ptn=X.U16(0xC)) > 4096) return;
	var infolen = X.U24(0x0E), smpsz = [], smpnsz = [];
