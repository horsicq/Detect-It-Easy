function isGlobalTracker() {
//ref https://ay.strangled.net/Ay_Emul30.src.7z/Players.pas -> FoundGTR
	if(X.Sz() < 296) return;
	p = 0x29; var adr = X.U16(5), mp = 0, old = X.U16(p-2)-adr, t;
	for(i=0; i < 14; i++) { t = X.U16(p)-adr; //sample ptrs check
		if(t-old < 6 || (t-old-2) % 4 || t > X.Sz()) return;  p += 2; old = t }
	old = X.U16(p)-adr; p += 2;
	for(i=0; i < 15; i++) { t = X.U16(p)-adr; //ornament ptrs check
		if(t-old < 3 || t > X.Sz() || t-old != 2+X.U8(old+1) /* orn. loop end */) return;  p += 2; old = t }
	sz = t+2+X.U8(t+1); //it's always the ornaments that come last, so we'll take the last one
	old = X.U16(p)-adr; p += 2;
	for(i=1; i < 32*3; i++) { t = X.U16(p)-adr; //pattern ptrs check
		if(t-old < 3 || t > X.Sz()) return;  p += 2; old = t }
	ord = X.U8(p++); lp = X.U8(p++); if(lp > ord) return;
	for(i=0; i < ord; i++) if(X.U8(p++) % 6) return; // all positions must be divisible by 6
	if(X.U16(0x65) != p) return; // start of PatChns; must match the pointer to PatChns[0]
	return true
}
