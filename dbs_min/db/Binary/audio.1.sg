		smpnsz[i] = X.U8(p); if(!(X.U8(p) & 0x80)) { smp++; smpsz[i] = X.U24(p+1); p += 3 } }
	title = X.UCSD(p); p += 1+X.U8(p);
	var ac = X.readBytes(p,8); p += 8; if(p > X.Sz()) return;
