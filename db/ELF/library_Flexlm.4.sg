meta("library", "");

function detect() {
    var nOffset = ELF.findSignature(0, ELF.getSize(), "'@(#) F'");
    if (nOffset != -1) {
        sName = ELF.getString(nOffset, 200).match(/\w\S*/i);
        sVersion = ELF.getString(nOffset, 200).match(/\d\S*/i);
        bDetected = true;
    }

    return result();
}