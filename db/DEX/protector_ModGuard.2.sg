meta("protector", "ModGuard");

function detect() {
    if (DEX.isDexStringPresent("ModGuard - Protect Your Piracy v1.2 by ill420smoker")) {
        sVersion = "v1.2";
    } else if (DEX.isDexStringPresent("ModGuard - Protect Your Piracy v1.3 by ill420smoker")) {
        sVersion = "v1.3";
    }

    return result();
}