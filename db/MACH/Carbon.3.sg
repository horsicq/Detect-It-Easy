init("library","Carbon");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(MACH.isLibraryPresent("Carbon"))
    {
        bDetected=1;
    }
    else if(MACH.isLibraryPresent("CarbonCore"))
    {
        bDetected=1;
    }

    return result(bShowType,bShowVersion,bShowOptions);
}