function detect(bShowType, bShowVersion, bShowOptions) {
    if (Binary.compare("e9$$$$fa be....8d0e....2bcee8$$$$5051ba....8bda301cac32c35183c4..b9")) {
        sOptions = "1996";
        bDetected = true;
    }

    return result(bShowType, bShowVersion, bShowOptions);
}