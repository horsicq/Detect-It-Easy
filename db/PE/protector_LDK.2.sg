meta("protector", "SafeNet Sentinel LDK");

function detect() {
    if (PE.getNumberOfSections() == 4 &&
        PE.isSectionNamePresent(".AKS1") &&
        PE.isSectionNamePresent(".AKS2") &&
        PE.isSectionNamePresent(".AKS3") &&
        PE.isSectionNamePresent(".rsrc")) {
        bDetected = true;
    }

    return result();
}