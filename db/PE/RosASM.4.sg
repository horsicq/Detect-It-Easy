init("compiler", "RosASM");

includeScript("RosASM");

function detect() {
    if (bRosASM) {
        sVersion = PE.getCompilerVersion();
        sOptions = PE.getGeneralOptionsEx(); // RosASM doesn't have a linker
        bDetected = true;

        // Correct version
        if (PE.getMajorLinkerVersion() > 3) {
            sVersion += "*";
        }
    }

    _setLang("ASMx" + (PE.is64() ? "64" : "86"), bDetected);

    return result();
}