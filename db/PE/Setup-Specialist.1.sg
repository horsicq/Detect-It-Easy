function detect(bShowType,bShowVersion,bShowOptions)
{
    if(PE.compareEP("6a..68........e8........bf........8bc7e8........8965..8bf4893e56ff15"))
    {
        if(PE.compareOverlay("'TGCF'"))
        {
            sVersion="4.0.x";
            bDetected=1;
        }
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
