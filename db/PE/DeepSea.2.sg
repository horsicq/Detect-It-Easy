// Detect It Easy: detection rule file

init("protector", "DeepSea");

function detect() {
    if (PE.isNET()) {
        var nOffset = PE.findString(PE.section[0].FileOffset, PE.section[0].FileSize, "DeepSeaObfuscator");
        if (nOffset != -1) {
            if (PE.compare("'Evaluation'", nOffset + 18)) {
                sVersion = "4.X";
                sOptions = "Evaluation";
            }
            bDetected = true;
        }
    }

    return result();
}