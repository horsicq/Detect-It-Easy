// https://learn.microsoft.com/en-us/windows/win32/direct3d9/directxfilecreate
meta("library", "D3DXOF");

function detect() {
    if (PE.isLibraryPresent("d3dxof.dll")) {
        bDetected = true;
    }

    return result();
}