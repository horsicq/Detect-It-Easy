init("installer", "SQX Archive Installer 2002");

function detect() {
    if (PE.compareEP("558bec83ec44") && PE.isSectionNamePresent(".data") && PE.findString(PE.section[".data"].FileOffset, PE.section[".data"].FileSize, "SFX kann nicht gestartet werden") != 1) {
        bDetected = true;
    }

    return result();
}