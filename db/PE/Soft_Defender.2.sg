// Detect It Easy: detection rule file

init("protector", "Soft Defender");

function detect() {
    if (PE.compareEP("74$$74$$78$$68A2AF470159E8")) { // 07  1F  0F
        if (PE.compareEP("BA010000", 72)) {
            sVersion = "1.0-1.1";
            bDetected = true;
        } else if (PE.compareEP("BE010000", 73)) {
            sVersion = "1.12";
            bDetected = true;
        } else {
            sVersion = "1.x";
        }
        bDetected = true;
    }

    return result();
}