init("protector","Kryptonit");

function detect(bShowType,bShowVersion,bShowOptions)
{
    nOffset=0x0093;
    if (PE.compareEP("558BEC"))
    {
        if (PE.compare("'Krypton'",nOffset))
        {
            sVersion="0.1-0.2";
            bDetected=1;
        }
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
