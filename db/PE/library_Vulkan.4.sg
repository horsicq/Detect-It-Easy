meta("library", "Vulkan");

function detect() {
    if (PE.isLibraryPresentExp(/[V|v]ulkan/i)) {
        bDetected = true;
    } else if (PE.isExportFunctionPresentExp(/VULKAN/)) {
        bDetected = true;
    }

    return result();
}