init("protector", "VaySoft PDF to EXE Converter");

function detect() {
    if (PE.compareEP("558BECB9")) {
        if (PE.compareOverlay("255B484220322A")) {
            sOptions = "PDF Adv.Xor Ovl.";
            sVersion = 6.82;
            bDetected = true;
        }
    }

    return result();
}