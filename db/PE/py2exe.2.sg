// Detect It Easy: detection rule file

meta("packer", "py2exe");

function detect() {
    for (var i = 0; i < PE.getNumberOfResources(); i++) {
        // py2exe magic number
        if (PE.compare("12345678", PE.resource[i].Offset)) {
            bDetected = true;
            break;
        }
    }

    sLang = "Python";

    return result();
}