// Detect It Easy: detection rule file
// Author: BJNFNE <bjnfne@web.de>

// https://github.com/llvm/llvm-project
meta("compiler", "LLVM");

function detect() {
    if (PE.section[".buildid"] || PE.compare("'LLD PDB.'", 0x0a27)) {
        sOptions = "LLD compiled";
        bDetected = true;
    }

    sLang = "C/C++";

    return result();
}