// https://wiki.scummvm.org/index.php?title=DEV7_Information
meta("game engine", "DEV7");

function detect() {
    if (PE.getVersionStringInfo("ProductName") == "Dev7 VM v1.30b") {
        sVersion = "1.30b";
        bDetected = true;
    } else if (PE.getVersionStringInfo("ProductName") == "Loader 7 v1.10a") {
        sVersion = "1.10a";
        bDetected = true;
    } else if (PE.getVersionStringInfo("ProductName") == "Dev7 VM v1.30a") {
        sVersion = "1.30a";
        sOptions = "Антошка 3"
        bDetected = true;
    } else if (PE.getVersionStringInfo("ProductName") == "Dev7 VM v1.30") {
        sVersion = "1.30";
        bDetected = true;
    }
    return result();
}