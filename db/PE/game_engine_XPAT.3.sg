// https://wiki.multimedia.cx/index.php/Engines/XPAT
meta("game engine", "XPAT");

function detect() {
    if (PE.getVersionStringInfo("FileDescription") === "Xpat Runtime Engine") {
        sVersion = "1.0.0.1";
        sOptions = "1995";
        bDetected = true;
    }

    return result();
}