meta("protection", "SoftLok dongle reference");

function detect() {
    var nSize = PE.isOverlayPresent() ? PE.getOverlayOffset() : PE.getSize();

    if (nSize > 50000000)
        nSize = 50000000;

    if (PE.findSignature(0, nSize, "5c5c2e5c'MNXX'") !== -1 || PE.findSignature(0, nSize, "5c5c2e5c'SoftLok'") !== -1) {
        bDetected = true;
    }

    return result();
}