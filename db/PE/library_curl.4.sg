meta("library", "Curl");

function detect() {
    if (PE.isLibraryPresentExp(/libcurl/i)) {
        bDetected = true;
    } else if (PE.isExportFunctionPresentExp(/^curl_easy_/)) {
        sOptions = "static"; // EAT
        bDetected = true;
    }

    return result();
}