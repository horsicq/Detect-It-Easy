init("System","ReactOS PE file");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(PE.section[".rossym"])
    {
        if (PE.section[".rossym"].Characteristics==0x42000802)
        {
            sVersion="v0.3";
            bDetected=1;
        }
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
