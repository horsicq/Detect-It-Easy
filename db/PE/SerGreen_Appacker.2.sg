// Detect It Easy: detection rule file

meta("packer", "SerGreen Appacker");

function detect() {
    if (PE.isNet()) {
        if (PE.isOverlayPresent()) {
            if (PE.compareOverlay("'<SerGreen>'")) {
                sVersion = "1.XX";
                bDetected = true;
            }
        }
    }

    return result();
}