// https://wiki.scummvm.org/index.php?title=Wintermute
meta("game engine", "Wintermute");

function detect() {
    if (PE.getVersionStringInfo("FileDescription") == "Wintermute Engine Runtime") {
        bDetected = true;
    }

    return result();
}