// https://github.com/Eronana/packer
meta("packer", "Eronana");

function detect() {
    if (PE.isSectionNamePresent(".packer") && PE.compareEP("60 68 00 ?? ?? ?? E8 17 FC FF FF 89 45 FC 61 FF 65 FC 00 00 00 00 00 00 00 00 00")) {
        bDetected = true;
    }

    return result();
}