init("library", "Microsoft C/C++ Runtime");

function detect() {
    if (PE.isLibraryPresent("vcruntime140.dll") ||
        PE.isLibraryPresent("vcruntime140_1.dll")) {
        bDetected = true;
        sOptions = "dynamic";
    }

    return result();
}