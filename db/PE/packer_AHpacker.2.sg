// Detect It Easy: detection rule file

meta("packer", "AHpacker");

function detect() {
    const epackSectionName = "!EPack";

    if (PE.compareEP("6068........B8........FF1068........50B8........FF1068........6A40FFD08905") &&
        PE.section[0].Name !== epackSectionName &&
        PE.section[PE.nLastSection].Name !== epackSectionName) {
        sVersion = "0.1";
        bDetected = true;
    }

    return result();
}