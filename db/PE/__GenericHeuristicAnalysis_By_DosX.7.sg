            var methodName = parts[2].split('$')[0];

            // Constructor: @Class@$bctr$...
            if (methodName === '$bctr') {
                return parts[1]; // Return class name
            }

            // Destructor: @Class@$bdtr$...
            if (methodName === '$bdtr') {
                return '~' + parts[1]; // Return destructor name
            }

            // Regular method
            return methodName;
        }
    }

    return itemPattern;
}



/**
