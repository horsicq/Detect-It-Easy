// https://gpuopen.com/amd-gpu-services-ags-library/
meta("library", "AMD Graphical Services");

function detect() {
    if (PE.isLibraryPresentExp(/^amd_ags_x(86|64)/i)) {
        bDetected = true;
    }

    return result();
}