function detect(bShowType,bShowVersion,bShowOptions)
{
    if(PE.compareEP("83ec..53555657e8........6a..5b391d........8bf37e..8b3d........a1........8b....8a08"))
    {
        if(PE.compareOverlay("'ExcelsiorII1'"))
