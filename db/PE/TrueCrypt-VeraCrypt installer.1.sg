init("installer","TrueCrypt-VeraCrypt installer");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if((PE.compareEP("E8....0000E9"))&&(PE.compareOverlay("..'CINSTRT'")))
    {
        sVersion="1.X-7.x";
        bDetected=1;
    }

    return result(bShowType,bShowVersion);
}
