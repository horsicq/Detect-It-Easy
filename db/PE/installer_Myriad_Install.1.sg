meta("installer", "Myriad Install");

function detect() {
    if (PE.compareEP("5589e5535683ec..55b8........505068........64ff35........648925........68........e8")) {
        if (PE.findSignature(PE.getOverlayOffset(), 0x100, "01006215") != -1) {
            bDetected = true;
        }
    }

    return result();
}