// https://www.nuget.org/packages/Antlr
meta("library", "Antlr");

function detect() {
    if (PE.isNetObjectPresent("Antlr3.Runtime")) {
        sVersion = "3.X";
        bDetected = true;
    } else if (PE.isNetObjectPresent("Antlr4.Runtime")) {
        sVersion = "4.X";
        bDetected = true;
    }

    return result();
}