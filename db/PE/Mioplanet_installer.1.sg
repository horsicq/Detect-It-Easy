meta("installer", "Mioplanet installer");

function detect() {
    if (PE.compareEP("eb$$a1........c1e0..a3........526a..e8........8bd0e8........5ae8")) {
        if (PE.compareOverlay("'[(*|*)]MZ'")) {
            bDetected = true;
        }
    }

    return result();
}