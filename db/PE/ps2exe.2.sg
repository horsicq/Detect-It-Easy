// https://github.com/MScholtes/PS2EXE
init("packer", "PS2EXE");

function detect() {
    bDetected = PE.isNetUStringPresent("^-([^: ]+)[ :]?([^:]*)$");

    _setLang("PowerShell", bDetected);

    return result();
}