init("installer", "Aeco Systems installer");

function detect() {
    if (PE.compareEP("64a1........558bec6a..68........68........50a1........648925........8b0d........83ec..8901") &&
        PE.compareOverlay("'EWETAR DATA FILE!! VER 1.00 <<<>>>'")) {
        bDetected = true;
    }

    return result();
}