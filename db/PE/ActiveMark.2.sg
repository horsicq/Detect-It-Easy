init("protector", "ActiveMARK");

function detect(bShowType, bShowVersion, bShowOptions) {
    if (PE.compareEP("8925........EB")) {
        bDetected = true;
    } else if (PE.compareOverlay("00'TMSAMVOH'")) {
        sVersion = "5.X";
        bDetected = true;
    }

    return result(bShowType, bShowVersion, bShowOptions);
}