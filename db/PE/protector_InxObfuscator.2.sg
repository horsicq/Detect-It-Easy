meta("protector", "Inx Obfuscator");

function detect() {
    if (PE.isNet() && PE.findSignature(0x00, PE.getSize() - PE.getOverlaySize(), "0013'Inx .NET Obfuscator'0000") != -1) {
        bDetected = true;
    }

    return result();
}