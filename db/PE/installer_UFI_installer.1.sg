meta("installer", "UFI installer");

function detect() {
    if (PE.compareEP("558BEC") && PE.resource["BINFILE"] && PE.resource["BINFILE_2"]) {
        sVersion = "1.X, Flash";
        bDetected = true;
    }

    return result();
}