// https://wiki.multimedia.cx/index.php/Engines/Atlas
meta("game engine", "Atlas");

function detect() {
    if (PE.getVersionStringInfo("ProductName") == "Atlas Game Engine") {
        bDetected = true;
    }

    return result();
}