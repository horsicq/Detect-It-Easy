meta("extender", "EMX DOS Extender");

function detect() {
    if (MSDOS.compareEP("b8....8ed88c06....e8....e8....e8....e8")) {
        sVersion = "0.8f-0.9d";
        bDetected = true;
    } else if (MSDOS.compareEP("b8....8ed88cd88ed0bc....e8....3c..72..26a1")) {
        sVersion = "0.8f";
        bDetected = true;
    }

    return result();
}