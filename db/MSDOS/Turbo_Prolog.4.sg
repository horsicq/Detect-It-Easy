init("compiler", "Turbo Prolog");

function detect() {
    if (MSDOS.compareEP("fab8....8ed8b8....a3....8ed0bc....fb8c06....268e06....8c06....b8....a3....9a")) {
        sVersion = "2.0";
        sOptions = "1986-88";
        bDetected = true;
    }

    _setLang("Prolog", bDetected);

    return result();
}