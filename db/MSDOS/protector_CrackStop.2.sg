meta("protector", "CrackStop");

function detect() {
    if (MSDOS.compareEP("B4..BB....B9....8BECCD21FAFC03....05....FFE0")) {
        sVersion = "1.X";
        bDetected = true;
    } else if (MSDOS.compareEP("bd....be....ffe6")) {
        sVersion = "1.0b";
        bDetected = true;
    }

    return result();
}