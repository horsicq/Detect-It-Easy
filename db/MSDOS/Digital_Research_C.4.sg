init("compiler", "Digital Research C");

function detect() {
    if (MSDOS.compareEP("eb$$0e17bc....a1....8cdb0e1f891e....a3....8cc80306....a3....b9....2bdb8bf3bf....8cc803050306")) {
        sVersion = "1.1";
        sOptions = "1983";
        bDetected = true;
    }

    _setLang("C", bDetected);

    return result();
}