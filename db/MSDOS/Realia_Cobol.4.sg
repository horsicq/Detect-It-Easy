init("compiler", "Realia Cobol");

function detect() {
    if (MSDOS.compareEP("9a########9a########3c..75..8ed38be22bdb2bd2cb")) {
        sVersion = "4.x";
        sOptions = "1984-92 by Realia, Inc.";
        bDetected = true;
    }

    _setLang("Cobol", bDetected);

    return result();
}