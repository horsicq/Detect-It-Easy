meta("extender", "WDos/X");

function detect() {
    if (MSDOS.compare("'WDOSX'", 52)) {
        sVersion = MSDOS.getString(58, 4);
        sOptions = "1996-1999 by Michael Tippach";
        bDetected = true;
    }

    return result();
}