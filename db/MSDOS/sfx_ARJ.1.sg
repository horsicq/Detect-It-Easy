// Detect It Easy: detection rule file

meta("sfx", "ARJ");

function detect() {
    if (MSDOS.compare("'RJSX'", 0x1c)) {
        sVersion = "old";
        bDetected = true;
    } else if (MSDOS.isSignaturePresent(0, Math.min(1000, MSDOS.getSize()), "'aRJsfX'")) {
        bDetected = true;
    }

    return result();
}