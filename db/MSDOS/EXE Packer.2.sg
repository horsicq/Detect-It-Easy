    {
        sVersion="7.0";
        bDetected=1;
    }
    else if(MSDOS.compareEP("EB$$1E068CC383....0E1F011E....011E....BE....B9....FCAD3BC174"))
    {
        sVersion="7.0";
        bDetected=1;
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
