meta("self-displayer", "GIFEXE");

function detect() {
    if (MSDOS.compareEP("ba....2e8916....b430cd218b2e....8b1e....8edaa3....8c06") &&
        MSDOS.compareOverlay("'GIF87a'") &&
        MSDOS.isSignaturePresent(MSDOS.getSize()-6, 6, "'GIFEXE'")) {
            sVersion = "1.0";
            sOptions = "1990, 1991 by Steve Enns and Dan Magosse";
            bDetected = true;
    }
    
    return result();
}