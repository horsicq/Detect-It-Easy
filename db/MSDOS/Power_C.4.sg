init("compiler", "Power C");

function detect() {
    if (MSDOS.compareEP("b8....05....b1..d3e88cca03c28b0e....8cdb8ed8891e....890e....8ec0ba....1e8cc88ed8b8....cd21588ed8bf....33c0")) {
        sVersion = "2.0.x";
        sOptions = "1989-91 by Mix Software";
        bDetected = true;
    }

    _setLang("C", bDetected);

    return result();
}