init("packer","protector");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(MSDOS.compareEP("bc....c3") && MSDOS.compare("'FCP/IV'", MSDOS.getEntryPointOffset()-6))
    {
        sOptions="by FCP/IV";
        bDetected=1;
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
