meta("protector", "COMPROTECT");

function detect() {
    if (MSDOS.compareEP("b8....15....72..8ac4bb....50515253555657061e50b8....58eb")) {
        sVersion = "2.10";
        sOptions = "1988-95 by Ralph Roth (converted to exe)";
        bDetected = true;
    }

    return result();
}