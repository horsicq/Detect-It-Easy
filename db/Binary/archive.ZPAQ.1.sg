init("archive","ZPAQ Compressed Archive (.ZPAQ)");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(Binary.getSize()>=9)
    {
	
        if(Binary.compare("'7kSt'") && Binary.compare("'zPQ'",0x0D))
        {
            bDetected=true; 
        }
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
