init("archive","");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(Binary.getSize()>=9)
    {
        if(Binary.compare("89'LZO'000D0A1A0A"))
            bDetected=true;
            sName = "LZOP compressed data";
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
