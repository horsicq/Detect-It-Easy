// https://moddingwiki.shikadi.net/wiki/Human_Machine_Interfaces_MIDI_Format
meta("audio", "Human Machine Interfaces");

function detect() {
    if (Binary.compare("'HMI-MIDISONG061595'0000000000000000000000000000000000000000ff00ffffffffff01")) {
        sVersion = "2.0";
        sOptions = "MIDI";
        bDetected = true;
    }

    return result();
}