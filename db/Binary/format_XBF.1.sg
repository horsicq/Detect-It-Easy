// https://learn.microsoft.com/en-us/uwp/api/windows.ui.xaml.markup.xamlbinarywriter?view=winrt-26100
meta("format", "XBF");

function detect() {
    if (Binary.compare("'XBF'")) {
        var id = Binary.getString(68, 32).trim();

        if (/^[A-Z0-9]{32}/.test(id)) {
            if (X.isVerbose()) sOptions = id;
            bDetected = true;
        }
    }

    return result();
}