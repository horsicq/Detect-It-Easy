meta("format", "VMWare Virtual Disk (VMDK)");

function detect() {
    if (Binary.compare("'KDMV'01") || Binary.compare("'KDMV'02") || Binary.compare("'KDMV'03")) {
        bDetected = true;
        sVersion = "v" + File.read_uint8(4)
    }

    return result();
}