// https://gtamods.com/wiki/IFP
meta("archive", "Animation file");

function detect() {
    if (Binary.compare("'ANP3'")) {
        var versionNumber = Binary.getString(3, 1).trim();

        if (/^[0-9]{1}/.test(versionNumber)) {
            if (X.isVerbose()) sVersion = versionNumber;
            bDetected = true;
        }
    }

    return result();
}