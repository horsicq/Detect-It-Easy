// https://www.gimp.org/
meta("image", "eXperimental Computing Facility (GIMP Image)");

function detect() {
    if (Binary.compare("67 69 6D 70 20 78 63 66")) {
        var versionNumber = Binary.getString(9, 4).trim();

        if (/^[a-z-0-9]{4}/.test(versionNumber)) {
            if (X.isVerbose()) sVersion = versionNumber;
            bDetected = true;
        }
    }

    return result();
}