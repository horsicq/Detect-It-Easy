// https://en.wikipedia.org/wiki/QuickTime_File_Format
meta("format", "QuickTime Movie");

function detect() {
    if (Binary.compare("........'moov'")) {
        var codec = Binary.getString(11, 5).trim();

        if (/^[a-z]{5}$/.test(codec)) {
            sOptions = codec;
            bDetected = true;
        }
    }

    return result();
}