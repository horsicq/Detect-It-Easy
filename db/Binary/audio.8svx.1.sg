// Detect It Easy signature file
// Author: KDSS-Research

init("audio", "8SVX");

includeScript("read");

function detect() {
    if (Binary.compare("'FORM'")) {
        if (Binary.findString(0, Binary.getSize(), "BODY") != -1 && Binary.findString(0, Binary.getSize(), "FORM") != -1 && Binary.findString(0, Binary.getSize(), "ANNO") != -1) {
            bDetected = true;
        }
    }

    return result();
}