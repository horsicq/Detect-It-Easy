        numtracks = File.read_uint32(8,_LE); if(!numtracks || numtracks > 256) return false;
        sz = 12+numtracks*4+numnotes*11;
        if((sz != File.getSize()) && (sz != File.getSize()+numnotes)) return false;
        return true
    }
