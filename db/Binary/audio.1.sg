        if(!File.compare("'SNX1'",a1)) return false; a1 += 4;
        d1 = File.read_uint32(a1,_BE); if(d1 >> 31) return false; a1 += 4;
        d1 = (d1+1) & 0xFFFFFFFE; a1 += d1;
