// https://wiki.scummvm.org/index.php?title=Gob/DEV7_Information#File_Formats
meta("format", "CTRK");

function detect() {
    if (Binary.compare("01000800000000000000")) {
        sOptions = File.cleanString(Binary.getString(0x0000001a, 11));
        bDetected = true;
    }

    return result();
}