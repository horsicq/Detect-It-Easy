init("format","");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(Binary.compare("'RIFF'........'CDR'"))
    {
        sName="CorelDraw graphics (.CDR)";
        bDetected=1;
    }
    else if(Binary.compare("'RIFF'........'AVI '"))
    {
        sName="AVI";
        bDetected=1;
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
