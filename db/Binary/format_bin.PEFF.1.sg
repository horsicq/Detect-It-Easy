// Detect It Easy: detection rule file

// https://en.wikipedia.org/wiki/Preferred_Executable_Format
// Rewritten by BJNFNE

meta("format", "Preferred Executable Format");

function detect() {
    if (Binary.compare("'Joy!peff'00000001")) {
        sVersion = "1.0";
        bDetected = true;
    } else if (Binary.compare("'Joy!peffpwpc'00000001")) {
        sOptions = "PowerPC";
        sVersion = "1.0";
        bDetected = true;
    }

    return result();
}