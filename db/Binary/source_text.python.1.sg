meta("source", "Python");

function detect() {
    var sText = Binary.getHeaderString();

    if ((/import\s/.test(sText)) && (/class\s/.test(sText)) && (/self/.test(sText))) {
        if (/\sdef\s/.test(sText)) {
            bDetected = true;
        }
    }

    sLang = "Python";

    return result();
}