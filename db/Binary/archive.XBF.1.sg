// https://thedustedarchives.com/de/monsterized-news/
meta("archive", "XBF");

function detect() {
    if (Binary.compare("'XBF'")) {
        var id = Binary.getString(68, 32).trim();

        if (/^[A-Z0-9]{32}/.test(id)) {
            if (X.isVerbose()) sOptions = id;
            bDetected = true;
        }
    }

    return result();
}