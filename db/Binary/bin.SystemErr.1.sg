// Used in Coktel Vision games

meta("format", "System_Err");

function detect() {
    if (Binary.compare("'System_Err'")) {
        var errorLine = Binary.getString(10, 48).trim();

        if (errorLine) {
            sVersion = errorLine;
            bDetected = true;
        }
    }
    return result();
}
