meta("format", "");

function detect() {
    if (Binary.compare("000003F300")) {
        bDetected = true;
        sName = "Amiga loadable file";
        // sOption(outSz(calcAmigaFileSize()), 'sz:');
    } else if (Binary.compare("000003E700")) {
        bDetected = true;
        sName = "Amiga object/library file";
    }

    return result();
}