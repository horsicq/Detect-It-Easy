// https://tcrf.net/Crazy_Machines
meta("format", "UCM");

function detect() {
    if (Binary.compare("'UCM1'02000000")) {
        sVersion = "1.0";
        sOptions = Binary.getString(0x0008);
        bDetected = true;
    }

    return result();
}