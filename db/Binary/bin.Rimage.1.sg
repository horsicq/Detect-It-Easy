// https://rimage.incom.de/en/
meta("format", "Rimage Image Server");

function detect() {
    if (Binary.compare("'Rimage Image Server Version: 8.6.604.1'", 0x00008373)) {
        sVersion = "8.6.604.1";
        bDetected = true;
    }

    return result();
}