// https://graphicsgale.com/
meta("format", "Gale (.GAL)");

function detect() {
    if (Binary.compare("'Gale102'..00000001")) {
        var frame = Binary.getString(20, 6).trim();

        if (/^[A-Za-z0-9]{6}/.test(frame)) {
            sVersion = "1.0.2";
            sOptions = frame;
            bDetected = true;
        }
    }

    return result();
}