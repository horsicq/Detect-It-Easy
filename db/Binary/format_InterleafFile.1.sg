// https://www.legoisland.org/wiki/Interleaf_File
meta("format", "Interleaf File");

function detect() {
    if (Binary.compare("52494646........'OMNI'")) {
        var metadata = Binary.getString(12, 4).trim();

        if (/^[A-Za-z]{4}/.test(metadata)) {
            if (X.isVerbose()) sOptions = metadata;
            bDetected = true;
        }

    }

    return result();
}