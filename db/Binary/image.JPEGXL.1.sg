init("image", "JPEG XL");

function detect() {
    if (Binary.getSize() >= 20) {
        if (Binary.compare("FF0A")) {
            sOptions = "codestream";
            bDetected = true;
        } else if (Binary.compare("0000000C4A584C200D0A870A")) {
            sOptions = "ISOBMFF-based container";
            bDetected = true;
        }
    }

    return result();
}