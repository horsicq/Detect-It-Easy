// https://www.psdevwiki.com/ps3/PS2_Savedata
meta("format", "PS2 Memory Card");

function detect() {
    if (Binary.compare("'Sony PS2 Memory Card Format 1.2.0.0'000000000000020200100000")) {
        sVersion = "1.2.0.0";
        bDetected = true;
    }

    return result();
}