// Detect It Easy: detection rule file

init("format", "Universal Binary");

function detect() {
    if (Binary.getSize() >= 8) {
        if (Binary.compare("CAFEBABE")) {
            var nMinor = Binary.readBEWord(4);
            var nMajor = Binary.readBEWord(6);
            if (nMinor) {
                bDetected = true;
            }
        }
    }

    return result();
}