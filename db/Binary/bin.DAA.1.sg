// https://github.com/horsicq/Detect-It-Easy/issues/45
// https://isc.sans.edu/diary/The+DAA+File+Format/25246
meta("format", "Direct Access Archive (.DAA)");

function detect() {
    if (Binary.compare("'DAA'0000000000000000000000004C")) {
        bDetected = true;
    }

    return result();
}