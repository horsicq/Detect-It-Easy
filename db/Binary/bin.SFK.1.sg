// https://www.reddit.com/r/audioengineering/comments/4208al/does_anyone_know_the_actual_format_of_sony_sfk/
meta("format", "SFK");

function detect() {
    if (Binary.compare("'SFPK'0100000040")) {
        bDetected = true;
    }

    return result();
}