// https://abandonware-magazines.github.io/magazines/magazine.html?m=Wiz&n=057&pagenumbers=1,12 (Used in My Fearless Friend made by Makh Shevet)
meta("format", "UPC");

function detect() {
    if (Binary.compare("'UPC'")) {
        var frc = Binary.getString(13, 3).trim();

        if (/^[A-Z]{3}$/.test(frc)) {
            if (X.isVerbose()) sOptions = frc;
            bDetected = true;
        }
    }

    return result();
}