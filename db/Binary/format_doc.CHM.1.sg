meta("format", "Microsoft Compiled HTML Help");

function detect() {
    if (Binary.compare("'ITSF'..000000")) {
        bDetected = true;
        sVersion = Binary.read_int32(4, _LE);
        sOption(Binary.read_uint32(0x14, _LE), "LangID: ");
    }

    return result();
}