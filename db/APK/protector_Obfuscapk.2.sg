meta("protector", "Obfuscapk");

function detect() {
    bDetected =
        APK.isArchiveRecordPresentExp(/assets\/lib\.arm(eabi|64)-v[0-9a-zA-Z]{2}\.[!-~]+\.so/) ||
        APK.isArchiveRecordPresentExp(/assets\/lib\.x86(_64)?\.[!-~]+\.so/);

    return result();
}