init("protector", "AndroidRepublic");

function detect() {
    if (APK.isArchiveRecordPresentExp(APK.isArchiveRecordPresent(/assets\/androidrepublic\.org\/(.*)\.png/))) {
        sVersion = "VIP";
        bDetected = true;
    } else if (APK.isArchiveRecordPresentExp("assets/emt.androidrepublic/config.png")) {
        bDetected = true;
    }

    return result();
}