meta("protector", "AlibabaProtection");

function detect() {
    bDetected =
        APK.isArchiveRecordPresent("lib/armeabi-v7a/libmobisec.so") ||
        APK.isArchiveRecordPresent("lib/armeabi/libmobisec.so") ||
        APK.isArchiveRecordPresent("lib/x86/libmobisec.so");

    return result();
}