meta("protector", "SecShell");

function detect() {
    bDetected =
        APK.isArchiveRecordPresent("assets/secData0.jar") ||
        APK.isArchiveRecordPresent("lib/armeabi/libSecShell-x86.so") ||
        APK.isArchiveRecordPresent("lib/armeabi/libSecShell.so");

    return result();
}