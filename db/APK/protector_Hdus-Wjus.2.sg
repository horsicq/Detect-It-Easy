meta("protector", "Hdus-Wjus");

function detect() {
    bDetected =
        APK.isArchiveRecordPresent("lib/armeabi/libhdus.so") ||
        APK.isArchiveRecordPresent("lib/armeabi/libwjus.so");

    return result();
}