// Detect It Easy: detection rule file

meta("format", "ISO9660");

function detect() {
    if (ISO9660.isVerbose()) {
        sName = ISO9660.getFileFormatName();
        sVersion = ISO9660.getFileFormatVersion();
        sOptions = ISO9660.getFileFormatOptions();

        if (sName) {
            bDetected = true;
        }
    }

    return result();
}