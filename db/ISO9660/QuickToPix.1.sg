// https://phantom.sannata.org/viewtopic.php?t=40730
meta("iso", "QuickToPix");

function detect() {
    if (Binary.compare("'MI QUICKTOPIX 2.00 BUILD 209_PHB_MPO'", 0x000081bf)) {
        sVersion = "2.00";
        sOptions = "Build: 209_PHB_MPO";
        bDetected = true;
    }

    return result();
}