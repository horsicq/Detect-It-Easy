meta("tool", "The Personal RomMaker");

function detect() {
    if (ISO9660.compare("'The Personal RomMaker (32-bit Windows 95 V4.10)'", 0x00000800)) {
        sVersion = "4.10";
        sOptions = "Windows 95 32-bit";
        bDetected = true;
    }

    return result();
}