// https://github.com/archlinux/archiso/blob/master/archiso/mkarchiso
meta("tool", "MKARCHISO");

function detect() {
    if (Binary.compare("'REPARED BY MKARCHISO'", 0x000081bf)) {
        bDetected = true;
    }

    return result();
}