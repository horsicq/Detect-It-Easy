// https://www.imgburn.com/
meta("tool", "ImgBurn");

function detect() {
    if (ISO9660.compare("'ImgBurn'", 0x00008373)) {
        sVersion = ISO9660.getString(0x0000837C, 7);
        bDetected = true;
    }

    return result();
}