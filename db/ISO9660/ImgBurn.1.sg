// https://www.imgburn.com/
meta("tool", "ImgBurn");

function detect() {
    if (Binary.compare("'ImgBurn v2.5.0.0'", 0x00008373)) {
        sVersion = "2.5.0.0";
        bDetected = true;
    } else if (Binary.compare("'ImgBurn v2.5.8.0'", 0x00008373)) {
        sVersion = "2.5.8.0";
        bDetected = true;
    }

    return result();
}