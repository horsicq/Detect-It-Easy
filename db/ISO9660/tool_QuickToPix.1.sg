// https://phantom.sannata.org/viewtopic.php?t=40730
meta("tool", "QuickToPix");

function detect() {
    if (ISO9660.compare("'MI_QUICKTOPIX'", 0x000081bf)) {
        bDetected = true;
    } else if (ISO9660.compare("'MI QUICKTOPIX 2.00 BUILD 209_PHB_MPO'", 0x000081bf)) {
        sVersion = "2.00";
        sOptions = "Build: 209_PHB_MPO";
        bDetected = true;
    }

    return result();
}