function detect() {
    if (Binary.compare("'9.2.0.2536'", 0x0000837c)) {
        sVersion = "9.2.0.2536";
        bDetected = true;
    } else if (Binary.compare("'9.7.5.3716'", 0x0000837c)) {
        sVersion = "9.7.5.3716";
        bDetected = true;
    } else if (Binary.compare("'9.7.6.3829'", 0x0000837c)) {
        sVersion = "9.7.6.3829";
        bDetected = true;
    }

    return result();
}